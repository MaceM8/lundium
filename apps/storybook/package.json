{
	"name": "@lundium/storybook",
	"version": "0.0.1",
	"private": true,
	"license": "UNLICENSED",
	"description": "Storybook application for documentation of our UI components",
	"main": "src/index.js",
	"devDependencies": {
		"@babel/core": "^7.7.7",
		"@lundium/components": "^0.0.1",
		"@storybook/addon-actions": "5.3.1",
		"@storybook/addon-info": "5.3.1",
		"@storybook/addon-knobs": "5.3.1",
		"@storybook/addon-options": "5.3.1",
		"@storybook/react": "5.3.1",
		"@storybook/theming": "5.3.1",
		"react": "^16.12.0"
	},
	"dependencies": {},
	"scripts": {
		"build": "yarn build:pdf && yarn build:ui",
		"build:ui": "build-storybook -o ../../build/storybook -c .storybook-ui",
		"build:pdf": "build-storybook -o ../../build/storybook -c .storybook-pdf",
		"start": "yarn start:ui",
		"start:ui": "start-storybook -p 8001 -c .storybook-ui",
		"start:pdf": "start-storybook -p 8004 -c .storybook-pdf"
	}
}
